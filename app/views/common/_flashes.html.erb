<% if flash.any? %>
    <% user_facing_flashes.each do |key, valuesomething| %>
            <div class="alert alert-<%= flash_class(key) %>">
                  <button type="button" name="button" class="close" data-dismiss="alert">x</button>
                  <%= valuesomething %>
            </div>
    <% end %>
<% end %>
